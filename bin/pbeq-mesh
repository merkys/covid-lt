#!/usr/bin/env python3

import math, sys

for line in sys.stdin:
    box = line.split()

    box[0] = math.floor(float(box[0]))
    box[1] = math.ceil(float(box[1]))
    box[2] = math.floor(float(box[2]))
    box[3] = math.ceil(float(box[3]))
    box[4] = math.floor(float(box[4]))
    box[5] = math.ceil(float(box[5]))

    print('NCLX', box[1]-box[0], 'XBCEN', (box[0]+box[1])/2, '-')
    print('NCLY', box[3]-box[2], 'YBCEN', (box[2]+box[3])/2, '-')
    print('NCLZ', box[5]-box[4], 'ZBCEN', (box[4]+box[5])/2, '-')
